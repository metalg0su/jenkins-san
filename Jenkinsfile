pipeline {
  agent any
  stages {
    stage('asdf') {
      parallel {
        stage('asdf') {
          steps {
            echo 'fdsa'
          }
        }
        stage('asdf') {
          steps {
            echo 'fdsa'
          }
        }
      }
    }
    stage('gg') {
      parallel {
        stage('gg') {
          steps {
            echo 'ggfdsa'
          }
        }
        stage('news') {
          steps {
            echo 'news'
          }
        }
      }
    }
  }
}